{
  "host_env_var_prefix": "DIRECTORY_UI_SUPPLIER",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "PORT",
      "SECRET_KEY",
      "RECAPTCHA_PUBLIC_KEY",
      "RECAPTCHA_PRIVATE_KEY",
      "UTM_COOKIE_DOMAIN",
      "CMS_URL",
      "CMS_SIGNATURE_SECRET"
    ],
    "optional": [
      "DEBUG",
      "ZENDESK_TICKET_SUBJECT",
      "GOOGLE_TAG_MANAGER_ID",
      "GOOGLE_TAG_MANAGER_ENV",
      "NOCAPTCHA",
      "SESSION_COOKIE_SECURE",
      "EXPORT_OPPORTUNITY_LOCALITIES",
      "SECURE_HSTS_SECONDS",
      "SECURE_SSL_REDIRECT",
      "FEATURE_MAINTENANCE_MODE_ENABLED"
    ]
  }
}
